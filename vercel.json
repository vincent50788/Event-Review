{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/app.js"
    },
    {
      "src": "/purchase",
      "dest": "/public/review.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
